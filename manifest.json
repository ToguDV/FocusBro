{
    "manifest_version": 3,
    "name": "FocusBro",
    "version": "1.0",
    "permissions": ["tabs", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "FocusBro"
    },
    "web_accessible_resources": [
      {
        "resources": ["blocked.html"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  